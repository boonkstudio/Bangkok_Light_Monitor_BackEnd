{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-names": "off",
        "camelcase": "off",
        "no-case-declarations": "off",
        "no-restricted-syntax": "off",
        "class-methods-use-this": "off",
        "global-require": "off",
        "guard-for-in": "off",
        "import/no-dynamic-require": "off",
        "import/no-extraneous-dependencies": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off"
    }
}
